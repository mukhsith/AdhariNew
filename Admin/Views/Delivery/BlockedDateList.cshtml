                            <th data-priority="10" class="">Block ID</th>
                            <th data-priority="2" class="">Block Start Date</th>
                            <th data-priority="3" class="">Block End Date</th>
                            <th data-priority="6" class="">Blocked on</th>
                            <th data-priority="5" class="">Blocked by</th>
                            <th data-priority="4" class="">Note</th>
                            <th data-priority="1" class="">Status</th>
                        </tr>
                    </thead>
                    <tbody class="border-secondary">
                    </tbody>
                </table>
            </div>
        </section>
        <!-- ========== End Card Block ========== -->
