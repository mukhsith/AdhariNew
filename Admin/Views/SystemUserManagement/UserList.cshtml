<header class="page-header">
    <h2>@SharedHtmlLocalizer["SystemUsers"]</h2>
    <div class="right-wrapper text-end me-3">
        <ol class="breadcrumbs">
            <li>
                <a href="/home">
                    <i class="bx bx-home-alt"></i>
                </a>
            </li>
            
        </ol>
         
    </div>
</header>
<div class="row">
    <div class="col">
        <!-- ========== Start Card Block ========== -->
        <section class="card mb-3 border border-secondary">
            <header class="card-header d-flex justify-content-between align-items-center p-3 border-secondary body-bg-secondary">
                <h2 class="card-title text-primary">@SharedHtmlLocalizer["SystemUsers"]</h2>
                <div class="card-actions d-flex position-static top-0 end-0">
                    <button class="btn btn-primary btn-sm " onclick="window.location = '/systemUserManagement/UserAddEdit';">@SharedHtmlLocalizer["AddNewUser"]</button>

                </div>
            </header>
            <div class="card-body ">
                <table class="table border-secondary align-middle mb-0 mobile-optimised datatable-default-" id="datatable-default-">
                    <thead>
                        <tr>
                            <th data-priority="1">@SharedHtmlLocalizer["ID"]</th>
                            <th data-priority="6">@SharedHtmlLocalizer["Status"]</th>
                            <th data-priority="1">@SharedHtmlLocalizer["Name"]</th>
                            <th data-priority="2">@SharedHtmlLocalizer["Mobile"]</th>
                            <th data-priority="4">@SharedHtmlLocalizer["Email"]</th>
                            <th data-priority="4">@SharedHtmlLocalizer["Gender"]</th>
                            <th data-priority="5">@SharedHtmlLocalizer["Role"]</th>
                            <th data-priority="5">@SharedHtmlLocalizer["DateCreated"]</th>
                            <th data-priority="5">@SharedHtmlLocalizer["DateModified"]</th>
                            <th data-priority="3">@SharedHtmlLocalizer["Actions"]</th>
                        </tr>
                    </thead>
                    <tbody class="border-secondary">
                        
                    </tbody>
                </table>
            </div>
        </section>
        <!-- ========== End Card Block ========== -->
    </div>
</div>

<!-- Modals -->
<!-- Vertically centered scrollable modal -->
<div class="modal fade" id="sort_modal" data-bs-keyboard="false" tabindex="-1" aria-labelledby="sort_modalLabel" aria-hidden="true">
    <div class="modal-dialog  modal-dialog-scrollable modal-dialog-centered">
        <div class="modal-content border-secondary">

            <div class="modal-header border-secondary body-bg-secondary">
                <h5 class="modal-title fw-bold text-primary" id="sort_modalLabel">@SharedHtmlLocalizer["DisplayOrder"]</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <!-- ========== Start Simple Input Field ========== -->

                <label for="" class="form-label">@SharedHtmlLocalizer["DisplayOrder"]</label>
                <input type="number" class="form-control form-control-lg rounded-4 border-secondary " name="" id="" aria-describedby="@SharedHtmlLocalizer["DisplayOrder"]" placeholder="" value="">


                <!-- ========== End Simple Input Field ========== -->
            </div>

            <div class="modal-footer">                
                <button id="btnSave" class="btn btn-primary btn-lg save-btn fw-bold">@SharedHtmlLocalizer["Save"]</button>                
                <button type="button" class="btn btn-warning" data-bs-dismiss="modal">@SharedHtmlLocalizer["Close"]</button>
            </div>
        </div>
    </div>
</div>


<!-- end: page -->
<script src="~/developer/js/SystemUserManagement/User_Datatable.js" asp-append-version="true"></script>